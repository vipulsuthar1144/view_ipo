{
  "name": "view_ipo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "env-cmd -f .env vite --host 0.0.0.0",
    "start:prod": "env-cmd -f .env.production vite",
    "build": "tsc && vite build",
    "build:prod": "tsc && env-cmd -f .env.production vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "preview:prod": "env-cmd -f .env.production vite preview",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.4",
    "@mui/material": "^6.1.4",
    "@mui/styles": "^6.1.4",
    "@mui/utils": "^6.1.4",
    "env-cmd": "^10.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/node": "^22.7.6",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  }
